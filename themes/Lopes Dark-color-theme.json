  "tokenColors": [
    {
      "scope": "variable",
      "settings": {
        "foreground": "#DFD5FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string constant.other.placeholder",
        "invalid.deprecated",
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1A77BC"
      }
    },
    {
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "entity.name.function",
        "punctuation.definition.interpolation.end.bracket.curly.scss",
        "punctuation.definition.interpolation.begin.bracket.curly.scss"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.interpolation.end.bracket.curly.scss",
        "punctuation.definition.interpolation.begin.bracket.curly.scss"
      ],
      "settings": {
        "foreground": "#E440FF"
      }
    },
